setenv env_addr "0x10400000"
setenv kernel_loadaddr "0x11000000"
setenv dtb_mem_addr "0x1000000"
setenv boot_start bootm ${kernel_loadaddr} - ${dtb_mem_addr}
setenv addmac 'if printenv mac; then setenv bootargs ${bootargs} mac=${mac}; elif printenv eth_mac; then setenv bootargs ${bootargs} mac=${eth_mac}; fi'
if fatload mmc 1 ${kernel_loadaddr} KERNEL; then if fatload mmc 1 ${env_addr} uEnv.ini; then env import -t ${env_addr} ${filesize}; run addmac; fi; if fatload mmc 1 ${dtb_mem_addr} ${dtb_name}; then run boot_start; fi; fi;
